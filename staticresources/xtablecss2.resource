.xtable table {
overflow:auto;
table-layout: fixed;
}

.xtable thead th {
text-align:center;
border-right: 2px solid #fff;
padding:2px;
background-color: #004080; 
table-layout: fixed;
color:#fff;
font-family:"Times New Roman";
}

}
.xtable tbody tr {
border-bottom: 2px solid #c0c0c0; 
}
.xtable tbody td {
color:#002084;
padding:2px;
text-align:left;
}
