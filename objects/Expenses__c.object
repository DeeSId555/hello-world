    <validationRules>
        <fullName>Prevent_Amount_Change_on_ReviewedExpense</fullName>
        <active>true</active>
        <description>Prevents staffing users changing the amount of an expense once it has been reviewed.</description>
        <errorConditionFormula>AND(
CONTAINS($Profile.Name , &quot;Staffing&quot;) ,
OR(
AND( ISCHANGED( Other_1__c ) , NOT( ISPICKVAL( Other_approved__c , &quot;Unreviewed&quot;) )),
AND( ISCHANGED( Other_2__c ) , NOT( ISPICKVAL( Telephone_approved__c, &quot;Unreviewed&quot;) )),
AND( ISCHANGED( Other_3__c ) , NOT( ISPICKVAL( Travel_approved__c , &quot;Unreviewed&quot;) )),
AND( ISCHANGED( item_4_amount__c ) , NOT( ISPICKVAL( Item_4_approved__c , &quot;Unreviewed&quot;) )),
AND( ISCHANGED( Item_5_amount__c ) , NOT( ISPICKVAL( Item_5_approved__c , &quot;Unreviewed&quot;) ))
))</errorConditionFormula>
        <errorMessage>You cannot change the amount  of an expense item once it has been reviewed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Blank_Item</fullName>
        <active>true</active>
        <description>If an Item has a value assigned to it, the corresponding amount field cannot be blank</description>
        <errorConditionFormula>OR(
AND( Other_1__c &gt;0.00,ISBLANK(TEXT( Item_1__c ))),
AND( Other_2__c &gt;0.00,ISBLANK(TEXT( Item_2__c ))),
AND( Other_3__c &gt;0.00,ISBLANK(TEXT( Item_3__c ))),
AND( item_4_amount__c &gt;0.00,ISBLANK(TEXT( Item_4__c ))),
AND( Item_5_amount__c &gt;0.00,ISBLANK(TEXT( Item_5__c ))))</errorConditionFormula>
        <errorMessage>If an expense item has an amount entered, an item type must also be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_post_payment_edits</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Paid__c = TRUE, ISCHANGED( Grand_Total__c ))</errorConditionFormula>
        <errorMessage>This expense claim has been paid. You can no longer change the approval fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_save_all_items_reviewed</fullName>
        <active>true</active>
        <description>Uses count fields to determine whether all of the items on the form have been reviewed.
Profile ID is System Administrator, so that records can be created through Clicktools</description>
        <errorConditionFormula>AND(  $User.ProfileId  &lt;&gt; &quot;00e80000000th5B&quot;,  $User.ProfileId  &lt;&gt; &quot;00eC0000001SpHf&quot;, $User.ProfileId  &lt;&gt; &quot;00eC0000001TNhw&quot;,reviewed_items__c &lt;  items__c)</errorConditionFormula>
        <errorMessage>You will not be able to save this Expenses record until you have approved or declined all of the claims.</errorMessage>
    </validationRules>
