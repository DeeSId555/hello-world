    <validationRules>
        <fullName>All_items_declined</fullName>
        <active>true</active>
        <description>Force user to enter reason why items were declined</description>
        <errorConditionFormula>OR(AND(ISPICKVAL(  Approve_all__c , &quot;Reject all&quot;), Other_1__c &gt; 0.00,  LEN( Item_1_reason_declined__c ) &lt; 10), AND(ISPICKVAL(  Approve_all__c , &quot;Reject all&quot;),  Other_2__c  &gt; 0.00,  LEN( Item_2_reason_declined__c ) &lt; 10), AND(ISPICKVAL(  Approve_all__c , &quot;Reject all&quot;),   Other_3__c   &gt; 0.00,  LEN( Item_3_reason_declined__c ) &lt; 10), AND(ISPICKVAL(  Approve_all__c , &quot;Reject all&quot;),  item_4_amount__c   &gt; 0.00,  LEN( Item_4_reason_declined__c ) &lt; 10), AND(ISPICKVAL(  Approve_all__c , &quot;Reject all&quot;),   Item_5_amount__c   &gt; 0.00,  LEN( Item_5_reason_declined__c ) &lt; 10))</errorConditionFormula>
        <errorMessage>Please write a few words in the &quot;reason declined&quot; fields to explain why each item was declined. It will help with future queries about the claim. Thanks.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Final_approval_date_and_PM</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR($User.UserRoleId = &quot;00EC0000001TEaC&quot;, $User.UserRoleId = &quot;00E80000001T6L0&quot;, $User.UserRoleId = &quot;00E80000001Sgji&quot;), ISBLANK( Final_approval_date__c ))</errorConditionFormula>
        <errorDisplayField>Final_approval_date__c</errorDisplayField>
        <errorMessage>Please put today&apos;s date in the field &quot;Final Approval Date&quot; to confirm that these expenses should be paid.</errorMessage>
    </validationRules>
