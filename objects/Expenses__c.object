        <errorConditionFormula>AND(ISPICKVAL( Other_approved__c , &quot;Declined&quot;),  Other_1__c &gt;0.00, LEN( Item_1_reason_declined__c ) &lt; 10)</errorConditionFormula>
        <errorDisplayField>Item_1_reason_declined__c</errorDisplayField>
        <errorMessage>Please write a few words to explain why this item was declined. It will help with future queries about the claim. Thanks.</errorMessage>
