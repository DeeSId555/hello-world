        <externalId>false</externalId>
        <formula>IF( transport_method__c = &quot;Private Transport&quot;,    

IF(  RecordTypeId = &quot;012C0000000QLXB&quot;, Arrival_TimeNew__c ,  Departure_timeNew__c )

,
IF(  RecordTypeId = &quot;012C0000000QLXB&quot;, LEFT(RIGHT(TEXT( Transport__r.Earliest_departure_time__c  ),9),5),
LEFT(RIGHT(TEXT( Transport__r.Latest_Arrival_time__c  ),9),5)
))</formula>
