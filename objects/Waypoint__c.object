    <fields>
        <fullName>Address_From__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	RIGHT( Name, 1 ) = &quot;C&quot;
	, IF(
		ISBLANK( Transport__r.Address_Override__c )
		, 
		Transport__r.From_Booking__r.Account_Name__c + &quot;, &quot; + Transport__r.From_Booking__r.Account_Address__c
		,
		Transport__r.Address_Override__c
	)
	, IF(
		ISBLANK( Transport__r.To_address_override__c )
		, 
		Transport__r.To_Booking__r.Account_Name__c + &quot;, &quot; + Transport__r.To_Booking__r.Account_Address__c
		,
		Transport__r.To_address_override__c
	) 
)</formula>
        <label>Address (From Account)</label>
