    <fields>
        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
NOT(ISBLANK(vehicle__c )),&quot;Optimised&quot;,
IF(
AND(
OR(
ISPICKVAL( Transport__r.Method_of_Transport__c , &quot;Walking&quot;),
ISPICKVAL( Transport__r.Method_of_Transport__c , &quot;Public Transport&quot;)
),
OR(
 Transport__r.Status__c = &quot;Approved (PM)&quot;, 
 Transport__r.Status__c = &quot;Approved (auto)&quot;
)
)
,&quot;Complete&quot;, &quot;Awaiting Approval&quot;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
