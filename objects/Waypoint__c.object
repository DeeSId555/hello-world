    </listViews>
    <listViews>
        <fullName>Before_Re_Optimisation_Arrivals</fullName>
        <columns>NAME</columns>
        <columns>vehicle__c</columns>
        <columns>Date_of_Travel__c</columns>
        <columns>Vehicle_Name__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>vehicle__c</field>
            <operation>contains</operation>
            <value>CAB</value>
        </filters>
        <filters>
            <field>Date_of_Travel__c</field>
            <operation>greaterOrEqual</operation>
            <value>7/20/2015</value>
        </filters>
        <filters>
            <field>Date_of_Travel__c</field>
            <operation>lessOrEqual</operation>
            <value>7/24/2015</value>
        </filters>
        <filters>
            <field>RECORDTYPE</field>
            <operation>equals</operation>
            <value>Waypoint__c.Delivery</value>
        </filters>
        <label>Before Re-Optimisation (Arrivals)</label>
        <sharedTo>
            <roleAndSubordinatesInternal>Head_of_Procurement_Innovation</roleAndSubordinatesInternal>
        </sharedTo>
    </listViews>
