    <validationRules>
        <fullName>Staff_unavailable_for_incorrect_training</fullName>
        <active>true</active>
        <description>This validation rule prevent s summer staff from being listed as attending training dates that are not relevant to their role and staff type as per the HR Training Event Policy 2014</description>
        <errorConditionFormula>OR( 
