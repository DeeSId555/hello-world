    <validationRules>
        <fullName>Actual_Days_Worked_Increments</fullName>
        <active>false</active>
        <description>Ensures that all actual days worked values are entered in increments of 0.5</description>
        <errorConditionFormula>AND( 

NOT(ISBLANK(days_worked__c)), 

RIGHT(text(floor(days_worked__c)) &amp;&quot;.&quot;&amp; lpad(text((days_worked__c-floor(days_worked__c))*100),1,&quot;0&quot;),1)&lt;&gt;&quot;0&quot;, 

RIGHT(text(floor(days_worked__c)) &amp;&quot;.&quot;&amp; lpad(text((days_worked__c-floor(days_worked__c))*100),1,&quot;0&quot;),1)&lt;&gt;&quot;5&quot;)</errorConditionFormula>
        <errorDisplayField>days_worked__c</errorDisplayField>
        <errorMessage>Please make sure the number of days worked is entered in half days 0.5) or whole days (1.0)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ad_Hoc_Date_Order</fullName>
        <active>true</active>
        <description>Ensures Ad Hoc days are entered in chronological order</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Ad_hoc_day_1__c )),

OR(
Ad_hoc_day_1__c &gt;= Ad_hoc_day_2__c,
Ad_hoc_day_1__c &gt;= Ad_hoc_day_3__c,
Ad_hoc_day_1__c &gt;= Ad_hoc_day_4__c,
Ad_hoc_day_1__c &gt;= Ad_hoc_day_5__c,

Ad_hoc_day_2__c &gt;= Ad_hoc_day_3__c,
Ad_hoc_day_2__c &gt;= Ad_hoc_day_4__c,
Ad_hoc_day_2__c &gt;= Ad_hoc_day_5__c,

Ad_hoc_day_3__c &gt;= Ad_hoc_day_4__c,
Ad_hoc_day_3__c &gt;= Ad_hoc_day_5__c,

Ad_hoc_day_4__c &gt;= Ad_hoc_day_5__c))</errorConditionFormula>
        <errorMessage>Please ensure you enter Ad Hoc days in chronological order</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ad_Hoc_Standard_Days_Completed</fullName>
        <active>true</active>
        <description>If a Job Placement is an Ad-Hoc record type, a number (max. 5) must be entered in the Standard Days field before the record can be saved</description>
        <errorConditionFormula>AND(
CONTAINS(RecordType.Name , &quot;Ad hoc&quot;),
OR(
ISBLANK(Standard_days_for_role__c ),
Standard_days_for_role__c &gt;5,
Standard_days_for_role__c =0))</errorConditionFormula>
        <errorDisplayField>Standard_days_for_role__c</errorDisplayField>
        <errorMessage>Ad hoc job placements must have a number between 1 and 5 entered on the &quot;Standard days for role&quot; field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ad_hoc_day_1_date_missing</fullName>
        <active>true</active>
        <description>Stops creation of Ad-hoc job placements without date entered for Day 1</description>
        <errorConditionFormula>AND(
CONTAINS(RecordType.Name,&quot;Ad hoc&quot;),
ISBLANK( Ad_hoc_day_1__c )
)</errorConditionFormula>
        <errorDisplayField>Ad_hoc_day_1__c</errorDisplayField>
        <errorMessage>Before you can create an Ad-hoc job placement, you must enter a date for the first day of the placement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ad_hoc_day_1venue</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(NOT(ISBLANK( Ad_hoc_day_1__c )),
ISBLANK(TEXT( Ad_hoc_Day_1_venue__c))),
AND(NOT(ISBLANK( Ad_hoc_day_2__c )), 
ISBLANK(TEXT( Ad_hoc_day_2_venue__c))),
AND(NOT(ISBLANK( Ad_hoc_day_3__c )), 
ISBLANK(TEXT( Ad_hoc_day_3_venue__c))))</errorConditionFormula>
        <errorMessage>Please Pick a value for- !!Ad hoc day venue Field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Adhoc_day_1_within_wave_dates</fullName>
        <active>true</active>
        <errorConditionFormula>OR( Ad_hoc_day_1__c &lt; PC_KO_Date__c , Ad_hoc_day_1__c &gt; RCA_End_Date__c , 
Ad_hoc_day_2__c &lt; PC_KO_Date__c , Ad_hoc_day_2__c &gt; RCA_End_Date__c , 
Ad_hoc_day_3__c &lt; PC_KO_Date__c , Ad_hoc_day_3__c &gt; RCA_End_Date__c , 
Ad_hoc_day_4__c &lt; PC_KO_Date__c , Ad_hoc_day_4__c &gt; RCA_End_Date__c , 
Ad_hoc_day_5__c &lt; PC_KO_Date__c , Ad_hoc_day_5__c &gt; RCA_End_Date__c)</errorConditionFormula>
        <errorMessage>The date must fall within the first and last day of the programme.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Allow_Job_reparenting_for_backup_staff</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 $User.ProfileId &lt;&gt; &quot;00e80000000th5B&quot;
 ,
 $User.ProfileId &lt;&gt; &quot;00eC0000001Sttg&quot;
 ,
 RecordTypeId &lt;&gt; &quot;012C0000000QJSF&quot;
 ,
 RecordTypeId &lt;&gt; &quot;012C0000000QJS6&quot;
 ,
 ISCHANGED( Job__c )
 ,
 NOT(TEXT( Status__c ) = &quot;On back-up list&quot;)
)</errorConditionFormula>
        <errorDisplayField>Job__c</errorDisplayField>
        <errorMessage>It is only possible to change the Job for back-up staff</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_RCA_4_day</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.Name = &quot;RCA 4 2014&quot;, OR( AND(ISBLANK( Job__r.Wave__r.RCA_4_Sat__c ), ISPICKVAL( RCA_day__c , &quot;Saturday&quot;)), AND(ISBLANK(  Job__r.Wave__r.RCA_4_Sun__c ), ISPICKVAL( RCA_day__c , &quot;Sunday&quot;))))</errorConditionFormula>
        <errorDisplayField>RCA_day__c</errorDisplayField>
        <errorMessage>You have selected the wrong RCA day. If you have selected &quot;Saturday&quot;, please change it to &quot;Sunday&quot; and vice versa</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_days_worked_dismissed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNULL( Job_Application__r.Date_dismissed__c )), days_worked__c &gt; (Days_worked_when_dismissed__c + 0.5), days_worked_confirmed__c = True ,  Withdrawal_dismissal_confirmed__c = False )</errorConditionFormula>
        <errorDisplayField>days_worked__c</errorDisplayField>
        <errorMessage>The system has calculated that this person, having been dismissed, has worked less than the number of days currently in the field &quot;Actual # days worked&quot;. Please enter the correct number of days.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_days_worked_withdrawn</fullName>
        <active>true</active>
        <description>Checks if the number of days in &quot;Actual days worked&quot; is higher than actual days worked after withdrawal</description>
        <errorConditionFormula>AND(NOT(ISNULL( Job_Application__r.Date_of_withdrawal_notification__c )), days_worked__c &gt; ( Days_worked_when_withdrawn__c  + 0.5), days_worked_confirmed__c = True ,  Withdrawal_dismissal_confirmed__c = False )</errorConditionFormula>
        <errorDisplayField>days_worked__c</errorDisplayField>
        <errorMessage>The system has calculated that this person, having withdrawn, has worked less than the number of days currently in the field &quot;Actual # days worked&quot;. Please enter the correct number of days.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirm_days_worked</fullName>
        <active>true</active>
        <description>Prevents payroll approval when &quot;#days worked confirmed&quot; is FALSE</description>
        <errorConditionFormula>AND(ISPICKVAL( Approved_for_payroll__c , &quot;Yes&quot;),  days_worked_confirmed__c = FALSE)</errorConditionFormula>
        <errorMessage>You must confirm the number of days worked, before you can approve the payroll for this person.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirm_withdrawal_dismissal_RCA</fullName>
        <active>false</active>
        <errorConditionFormula>AND(OR(NOT(ISBLANK(Job_Application__r.Date_dismissed__c )),NOT(ISBLANK( Job_Application__r.Date_of_withdrawal_notification__c ))),  days_worked__c = Standard_days_for_role__c ,  Withdrawal_dismissal_confirmed__c = False, OR( ISPICKVAL(  RCA_1_salary_approved__c , &quot;Yes&quot;), ISPICKVAL(  RCA_2_salary_approved__c , &quot;Yes&quot;), ISPICKVAL(  RCA_3_salary_approved__c , &quot;Yes&quot;), ISPICKVAL(  RCA_4_salary_approved__c , &quot;Yes&quot;)))</errorConditionFormula>
        <errorDisplayField>days_worked__c</errorDisplayField>
        <errorMessage>CRITICAL: You MUST enter the exact number of days that this person worked before they left the programme to ensure they are paid correctly. Tick &quot;Double confirm days worked&quot; to confirm that this number is correct.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_to_PV_only</fullName>
        <active>true</active>
        <description>Prevents the &quot;Resend contract&quot; checkbox from being ticked, if Role is not PV</description>
        <errorConditionFormula>AND(OR( RecordType.Name = &quot;RCA 1 2014&quot;,  RecordType.Name = &quot;RCA 2 2014&quot;,  RecordType.Name = &quot;RCA 3 2014&quot;,  RecordType.Name = &quot;RCA 4 2014&quot; ), NOT(ISPICKVAL( Role__c , &quot;Project Volunteer&quot;)),  Send_contract__c = TRUE)</errorConditionFormula>
        <errorDisplayField>Send_contract__c</errorDisplayField>
        <errorMessage>You can only send contracts to PVs on RCA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DEL_Prevent_double_payment_of_ad_hoc</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT(ISCHANGED( Total_salary_paid__c )), Total_salary_paid__c = TRUE, OR(Ad_hoc_payment_paid__c = TRUE,  Advance_paid__c = TRUE))</errorConditionFormula>
        <errorDisplayField>Ad_hoc_additional_payment__c</errorDisplayField>
        <errorMessage>The total amount has been paid to this person. You can longer tick &quot;Ad hoc payment paid&quot; or &quot;Advance paid&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enforce_Absence_Reason_and_Dates</fullName>
        <active>true</active>
        <description>If actual days for role NOT EQUAL to standard days for role, Absence 1 reason and Absence 1 dates must be filled in</description>
