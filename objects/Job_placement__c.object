AND(CONTAINS($UserRole.Name, &quot;Staffing&quot;), LEN(Reason_for_employment__c) &lt; 20 ,  CONTAINS(RecordType.Name, &quot;Ad hoc&quot;) ),  

AND(CONTAINS($UserRole.Name, &quot;Staffing&quot;), LEN(Reason_for_employment__c) &lt; 20  ,Job__r.Payroll_processing_meeting_date__c  &lt;   DATEVALUE(CreatedDate)  ),

