        <externalId>false</externalId>
        <formula>if( OR( ISPICKVAL( Status ,&quot;Attended&quot;), ISPICKVAL( Status ,&quot;Attended (recorded on website by YP)&quot;)),1, if(or(ISPICKVAL(Status , &quot;Responded - attending&quot;),ISPICKVAL(Status , &quot;Responded - attending &amp; bringing friend&quot;)),0,NULL ))</formula>
