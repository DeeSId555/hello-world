    <fields>
        <fullName>Total_Attendance_in_a_year__c</fullName>
        <externalId>false</externalId>
        <formula>IF( VALUE(TEXT(Meetings_attended_Nov_Dec__c))&gt;0, VALUE(TEXT(Meetings_attended_Nov_Dec__c)), 0 ) + 
IF( VALUE(TEXT(Meetings_attended_May_Jun__c))&gt;0, VALUE(TEXT(Meetings_attended_May_Jun__c)), 0)+ 
IF( VALUE(TEXT(Meetings_attended_Mar_Apr__c))&gt;0, VALUE( TEXT(Meetings_attended_Mar_Apr__c)), 0) + 
IF( VALUE(TEXT(Meetings_attended_Jul_Aug__c))&gt;0, VALUE(TEXT(Meetings_attended_Jul_Aug__c)), 0)+ 
IF( VALUE(TEXT( Meetings_attended_Dec__c ))&gt;0, VALUE(TEXT( Meetings_attended_Dec__c )), 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total # Attendance in a year</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
