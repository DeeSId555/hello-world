    <validationRules>
        <fullName>Constituency_website_enforcement</fullName>
        <active>false</active>
        <description>Only allows a constituency website address to be put in when Support on constituence website is selected in request</description>
        <errorConditionFormula>IF(AND(NOT(ISPICKVAL(Request__c,&quot;Support on constituency website&quot;)), NOT(ISBLANK( Constituency_website__c ))),TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>Request__c</errorDisplayField>
        <errorMessage>As you have put an address into constituency website you must select support on constituency website or remove the address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_attending_programme</fullName>
        <active>false</active>
        <description>Requires that the Date attending programme field is left blank if the MP has not agreed to attend the programme</description>
        <errorConditionFormula>IF(AND(ISPICKVAL( Visit_agreed__c ,&quot;No&quot;), NOT(ISBLANK(Date_of_visit__c ))),TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>Date_of_visit__c</errorDisplayField>
        <errorMessage>Please remove this date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Did_they_attend</fullName>
        <active>false</active>
        <errorConditionFormula>IF(AND(OR(TODAY()&lt; Date_of_visit__c,ISBLANK(Date_of_visit__c)) , ISPICKVAL(Did_they_attend__c,&quot;Yes&quot;)),TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>Did_they_attend__c</errorDisplayField>
        <errorMessage>Choose this option once the MP has attended the programme.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Endorsement_Quote_enforcement</fullName>
        <active>false</active>
        <description>Only allow Endorsement quote when request is selected</description>
        <errorConditionFormula>IF(AND(NOT(ISPICKVAL(Request__c,&quot;Endorsement Quote&quot;)), NOT(ISBLANK(Endorsement_Quote__c))),TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>Request__c</errorDisplayField>
