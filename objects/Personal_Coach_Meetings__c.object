    <validationRules>
        <fullName>Date_Location_Or_Time_of_Meeting_Blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(
ISBLANK(  Meeting_date__c ), 
ISBLANK(  Location__c  ), 
ISBLANK(  Time_of_meeting__c)))</errorConditionFormula>
        <errorMessage>You will not be able to save this record until you have put in the Date, Location and Time of this meeting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Meeting_Successful_after_meeting_date</fullName>
        <active>true</active>
        <description>Forces use to populate Meeting Successful? after the date of the meeting has passed.
This was added because users were not doing this.</description>
        <errorConditionFormula>AND(Today() &gt;= Meeting_date__c , NOT(ISPICKVAL (Meeting_successful__c , &quot;No&quot;)),NOT(ISPICKVAL( Meeting_successful__c , &quot;Yes&quot;)))</errorConditionFormula>
        <errorDisplayField>Meeting_successful__c</errorDisplayField>
        <errorMessage>Please select &quot;Yes&quot; or &quot;No&quot; from the &quot;Meeting Successful?&quot; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Meeting_summary</fullName>
        <active>true</active>
        <description>Require a meeting summary when the YP has attended a meeting</description>
        <errorConditionFormula>AND(ISPICKVAL( Meeting_attendance__c , &quot;Attended&quot;),ISBLANK( Meeting_summary__c ))</errorConditionFormula>
        <errorDisplayField>Meeting_summary__c</errorDisplayField>
        <errorMessage>Please provide a summary/outline of the meeting the YP attended</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Two_days_after_Meet_Summary_not_updated</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Meeting_date__c   &lt;=    TODAY() -  2, ISBLANK( Meeting_summary__c ), OR (NOT( ISCHANGED( Meeting_summary__c ) ) ))</errorConditionFormula>
        <errorMessage>In order to save this record you must update the meeting summary</errorMessage>
    </validationRules>
</CustomObject>
