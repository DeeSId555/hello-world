    <validationRules>
        <fullName>Hours_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(TEXT( Reason ))),  OR(ISPICKVAL(Department__c , &quot;Programmes - Summer prog&quot;), ISPICKVAL( Department__c , &quot;Staffing&quot;)),  Hours_required__c = 0.0)</errorConditionFormula>
        <errorDisplayField>Hours_required__c</errorDisplayField>
        <errorMessage>Please enter an estimate of how long it took you to investigate and resolve the case</errorMessage>
    </validationRules>
