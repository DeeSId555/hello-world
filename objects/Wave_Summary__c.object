    <fields>
        <fullName>Wave_Summary_Number__c</fullName>
        <externalId>false</externalId>
        <formula>IF(CONTAINS(Name, &quot;01&quot;), &quot;01&quot;,
IF(CONTAINS(Name, &quot;02&quot;), &quot;02&quot;,
IF(CONTAINS(Name, &quot;03&quot;), &quot;03&quot;,
IF(CONTAINS(Name, &quot;04&quot;), &quot;04&quot;,
IF(CONTAINS(Name, &quot;05&quot;), &quot;05&quot;,
IF(CONTAINS(Name, &quot;06&quot;), &quot;06&quot;,
IF(CONTAINS(Name, &quot;07&quot;), &quot;07&quot;,
IF(CONTAINS(Name, &quot;08&quot;), &quot;08&quot;,
IF(CONTAINS(Name, &quot;09&quot;), &quot;09&quot;,
IF(CONTAINS(Name, &quot;10&quot;), &quot;10&quot;,
IF(CONTAINS(Name, &quot;11&quot;), &quot;11&quot;,
IF(CONTAINS(Name, &quot;12&quot;), &quot;12&quot;,
NULL))))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Wave Summary Number</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Wave_number__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
(LEFT(RIGHT( Name ,2),1)=&quot; &quot;),
RIGHT(Name,1),
IF(
ISNUMBER(RIGHT(Name,2)),
RIGHT(Name,2),
LEFT(RIGHT(Name,3),2)
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Wave number</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
