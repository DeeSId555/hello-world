        <externalId>false</externalId>
        <formula>IF(
   ISBLANK( Inclusion_Detail__r.Contact__r.Assigned_Wave__c),
   IF(
      ISBLANK( Inclusion_Detail__r.Contact__r.Reserved_Wave__c),
      IF(
         ISBLANK( Inclusion_Detail__r.Contact__r.NCS_Application_Primary_Season__c),
         IF(
            ISBLANK( Inclusion_Detail__r.Contact__r.Season__c ),
            TEXT(YEAR( DATEVALUE( Inclusion_Detail__r.CreatedDate ))),
            IF(
               OR(
                  INCLUDES(Inclusion_Detail__r.Contact__r.Season__c,&quot;Summer 2016&quot;),
                  INCLUDES(Inclusion_Detail__r.Contact__r.Season__c,&quot;Autumn 2016&quot; )
                 ),
               &quot;2016&quot;,
               &quot;2015&quot;
               )
            ),
         RIGHT(Inclusion_Detail__r.Contact__r.NCS_Application_Primary_Season__c,4)
         ),
      TEXT(YEAR(Inclusion_Detail__r.Contact__r.Reserved_Wave__r.PC_KO_day__c) )
      ),
   TEXT(YEAR(Inclusion_Detail__r.Contact__r.Assigned_Wave__r.PC_KO_day__c) )
   )</formula>
