    <fields>
        <fullName>Age_on_31_Aug_of_campaign_year__c</fullName>
        <description>Calculates age on 31 August of campaign year. Currently set for 2016</description>
        <externalId>false</externalId>
        <formula>IF( 
MONTH(X31_August_current_year__c )&gt;MONTH(  Inclusion_Detail__r.Contact__r.Birthdate  ), 
YEAR(X31_August_current_year__c )-YEAR( Inclusion_Detail__r.Contact__r.Birthdate ), 
IF(AND(MONTH(X31_August_current_year__c ) = MONTH( Inclusion_Detail__r.Contact__r.Birthdate ), DAY(X31_August_current_year__c ) &gt;= DAY( Inclusion_Detail__r.Contact__r.Birthdate )), 
YEAR(X31_August_current_year__c )-YEAR( Inclusion_Detail__r.Contact__r.Birthdate ), 
(YEAR(X31_August_current_year__c )-YEAR( Inclusion_Detail__r.Contact__r.Birthdate ))-1) 
)</formula>
        <inlineHelpText>Calculates age on 31 August of campaign year. Currently set for 2016</inlineHelpText>
        <label>Age on 31 Aug of campaign year</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
