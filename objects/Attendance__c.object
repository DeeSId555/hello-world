        <fullName>Total_number_of_recorded_absences__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 

(Unauthorised_absences__c + Authorised_absences__c) *8 

- IF( Contact__r.Assigned_Wave__r.Autumn_wave__c = TRUE, 1.5,IF( Wave_Model__c =&quot;Pilot&quot;,2,2.5))*8 

- IF( Percentage_of_Days_Attended_HIDDEN__c &gt;=0.75,Authorised_Absences_Hours_Allowed_wf__c,0)

&lt; 0, 0, 


(Unauthorised_absences__c + Authorised_absences__c) *8 

- IF( Contact__r.Assigned_Wave__r.Autumn_wave__c = TRUE, 1.5,IF( Wave_Model__c =&quot;Pilot&quot;,2,2.5))*8 


- IF( Percentage_of_Days_Attended_HIDDEN__c &gt;=0.75,Authorised_Absences_Hours_Allowed_wf__c,0)
)</formula>
        <label>Total Hours required for 85%</label>
        <precision>18</precision>
        <required>false</required>
