    <validationRules>
        <fullName>Prevent_nonpilot_days_for_pilot_waves</fullName>
        <active>true</active>
        <description>Prevents users from entering an attendance status for days which aren&apos;t included on pilot waves, e.g. TC days 4 and 5</description>
        <errorConditionFormula>AND(Wave_Model__c = &quot;Pilot&quot;,

OR(
NOT(ISPICKVAL(TC_4__c,&quot;&quot; )),
NOT(ISPICKVAL(TC_5__c,&quot;&quot; )),
NOT(ISPICKVAL(RCD_3__c,&quot;&quot; )),
NOT(ISPICKVAL(RCD_4__c,&quot;&quot; ))))</errorConditionFormula>
        <errorMessage>This day is not part of the pilot wave model - please keep the following fields clear:

TC 4
TC 5
RCD 3
RCD 4</errorMessage>
    </validationRules>
