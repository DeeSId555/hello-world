    <validationRules>
        <fullName>AM_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AMs__c  &gt;  5</errorConditionFormula>
        <errorMessage>There are already 5 AMs/SAMs allocated to this wave.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APL_Validation</fullName>
        <active>true</active>
        <description>Prevents more than given amount of APLs being added to a Job</description>
        <errorConditionFormula>APL__c &gt;  Max_APLs__c</errorConditionFormula>
        <errorMessage>The position of APL has been filled for this wave. If you need to add an extra APL, please ask your manager to edit the &quot;Max APL&quot; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CF_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Community_Mentors__c &gt;  Max_Community_Mentors__c</errorConditionFormula>
        <errorMessage>The Community Facilitator positions have been filled for this wave. If you need to add more CFs, please ask your manager to edit the &quot;Max CFs&quot; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mentor_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Mentors__c  &gt;  Max_Mentors__c</errorConditionFormula>
        <errorMessage>The Mentor positions have been filled for this wave. If you need to add more Mentors, please ask your manager to edit the &quot;Max Mentor&quot; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_SW_place_on_Wave</fullName>
        <active>false</active>
        <description>Prevents SW from being added to a wave when there is no place for one.</description>
        <errorConditionFormula>AND( SW__c &gt; 1, ISPICKVAL( Wave__r.Place_available_for_Support_Worker__c , &quot;No&quot;))</errorConditionFormula>
        <errorMessage>This wave has no place for a Support Worker, so you cannot add one to this Job. Please speak to the Inclusion team for more info.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PL_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>PL__c  &gt; Max_PLs__c</errorConditionFormula>
        <errorMessage>The position of Programme Leader has been filled for this wave.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Practitioner_validation</fullName>
        <active>true</active>
        <errorConditionFormula>Practitioners__c &gt;  Max_Practitioners__c</errorConditionFormula>
        <errorMessage>The Practitioner roles have been filled for this wave. If you need to add another Practitioner, please ask your manager to edit the &quot;Max Practitioner&quot; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RM_validation</fullName>
        <active>true</active>
        <description>Checks that no more than 1 REs. Accomm Manager is added</description>
        <errorConditionFormula>Res_Accomm_Manager__c  &gt;  Max_RAM__c</errorConditionFormula>
        <errorMessage>The Residential Mentor positions have been filled for this wave. If you need to add another RM, please ask your manager to edit the field &quot;Max RM&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Support_work_place_filled_1place1YP</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Wave__r.Place_available_for_Support_Worker__c , &quot;Yes - 1 SW&quot;),  SW__c &gt; 1)</errorConditionFormula>
        <errorMessage>There are no places left for additional support workers. Please contact inclusion to speak to them about potentially taking an extra support worker on this wave</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Support_work_place_filled_1place2YP</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Wave__r.Place_available_for_Support_Worker__c , &quot;Yes - 1 SW for 2 YPs&quot;),  SW__c &gt; 1)</errorConditionFormula>
        <errorMessage>There are no places left for additional support workers. Please contact inclusion to speak to them about potentially taking an extra support worker on this wave</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Support_work_place_filled_2places</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Wave__r.Place_available_for_Support_Worker__c , &quot;Yes - 2 SWs&quot;),  SW__c &gt; 2)</errorConditionFormula>
        <errorMessage>There are no places left for additional support workers. Please contact inclusion to speak to them about potentially taking an extra support worker on this wave</errorMessage>
    </validationRules>
</CustomObject>
