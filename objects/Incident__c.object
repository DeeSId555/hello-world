    <startsWith>Vowel</startsWith>
    <validationRules>
        <fullName>Card_Type_Awarded_Required_Fields</fullName>
        <active>false</active>
        <errorConditionFormula>AND(OR(
ISBLANK( Date_that_PL_was_informed__c), 
ISBLANK( Time_that_PL_was_informed__c ), 
ISBLANK( Date_that_PM_was_informed__c ),
ISBLANK( Time_that_PM_was_informed__c)), 
OR(ISPICKVAL( Card_type__c , &quot;Yellow&quot;),
ISPICKVAL(Card_type__c , &quot;Red (2nd yellow awarded)&quot;),
 ISPICKVAL(Card_type__c , &quot;Red&quot;)
)
)</errorConditionFormula>
        <errorMessage>You will not be able to save this unless you have filled in: Date that PL was informed; Time that PL was informed; Date that PM was informed; Time that PM was informed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_YP_Left_must_be_null_unless</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
	AND(
		OR(
			ISPICKVAL(  Card_type__c, &apos;Red (2nd yellow awarded)&apos;)
			,
			ISPICKVAL( Card_type__c, &apos;Red&apos;)
		)
		,
		ISNULL( Date_left_the_programme__c)
	)
	,
	AND(
		OR(
			ISPICKVAL(  Card_type__c, &apos;Yellow&apos;)
			,
			ISPICKVAL( Card_type__c, &apos;Warning&apos;)
			,
			ISPICKVAL( Card_type__c, &apos;Other&apos;)
		)
		,
		NOT( ISNULL( Date_left_the_programme__c))
	)
)</errorConditionFormula>
        <errorMessage>You must enter a date the YP left the programme if they received a red card and can not enter a date if they have not received a red card.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Document_must_be_uploaded_for_disclosure</fullName>
        <active>false</active>
        <errorConditionFormula>AND( RecordTypeId = &apos;012C0000000QUKI&apos;,   ISBLANK( Document_URL__c ))</errorConditionFormula>
        <errorMessage>You must input a valid Document URL for disclosure incidents.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Highly_severe_incident_need_explanation</fullName>
        <active>true</active>
        <description>If the Severity of an incident is &apos;Major&apos; then an explanation of the</description>
        <errorConditionFormula>AND((ISPICKVAL( Severity_of_Injury__c ,&quot;Major (Person sent to Hospital)&quot;)),ISBLANK( Description_of_incident__c ))</errorConditionFormula>
        <errorMessage>Please enter a description of the Incident</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mental_Health_Incident</fullName>
        <active>true</active>
        <description>Delivery staff need to tick any one one the fields: Safeguarding and Disclosure or Mental Health or both</description>
        <errorConditionFormula>AND( RecordType.Id = &quot;012C0000000QUKI&quot;,Safeguarding_and_Disclosure__c = false,  Mental_Health__c  = False)</errorConditionFormula>
        <errorMessage>Please tick any one or both of the following boxes in the information section: Safeguarding/Disclosure AND/OR Mental Health?</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_What_Was_Lost_Restriction</fullName>
        <active>true</active>
        <description>Prevents users entering text in the &quot;If other, what was lost&quot; field if &quot;Other&quot; option is not selected in &quot;What was lost&quot; picklist</description>
        <errorConditionFormula>AND(ISPICKVAL(What_was_lost__c , &quot;Other&quot;)=FALSE,
  ISBLANK(If_other_what_was_lost__c) = FALSE)</errorConditionFormula>
        <errorDisplayField>If_other_what_was_lost__c</errorDisplayField>
        <errorMessage>This field should be blank unless &apos;Other&apos; is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_What_Was_Lost_RestrictionAnswerReq</fullName>
        <active>true</active>
        <description>Requires users to enter text in the &quot;If other, what was lost&quot; field if &quot;Other&quot; is selected for &quot;What was lost&quot;</description>
        <errorConditionFormula>AND(ISPICKVAL(What_was_lost__c , &quot;Other&quot;), 
ISBLANK(If_other_what_was_lost__c) = TRUE)</errorConditionFormula>
        <errorDisplayField>If_other_what_was_lost__c</errorDisplayField>
        <errorMessage>This field should contain text if &apos;Other&apos; is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reporting_Date_Required</fullName>
        <active>true</active>
        <description>Required user to enter date associated with reporting body</description>
        <errorConditionFormula>OR(

AND( Reported_to_PHAROS__c = TRUE ,ISBLANK( Date_Reported_to_PHAROS__c )),

AND( Reported_to_RIDDOR__c = TRUE ,ISBLANK( Date_Reported_to_RIDDOR__c )),

AND(  Reported_to_The_Charity_Commission__c  = TRUE ,ISBLANK(  Date_Reported_to_The_Charity_Commission__c )))</errorConditionFormula>
        <errorMessage>Please enter the date you reported this incident in the right-hand column</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_departure_day</fullName>
        <active>true</active>
        <description>Requires &quot;If not, what was their departure day?&quot; if &quot;Did YP leave the programme the same day?&quot; = &quot;No&quot;.</description>
        <errorConditionFormula>AND(ISPICKVAL(Did_YP_leave_the_programme_the_same_day__c, &quot;No&quot;), ISBLANK( If_not_what_was_their_departure_day__c ))</errorConditionFormula>
        <errorDisplayField>If_not_what_was_their_departure_day__c</errorDisplayField>
        <errorMessage>Please enter the date that the YP left the programme</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPMs_Incident_Escalation</fullName>
        <active>true</active>
        <description>Prevents non-SPMs  from checking the fields for reporting to PHAROS or The Charity Commission</description>
        <errorConditionFormula>AND(
NOT($User.ProfileId = &quot;00e80000000th5B&quot;),
NOT($User.ProfileId = &quot;00eC0000001TNhw&quot;),
NOT(CONTAINS( $UserRole.Name ,&quot;SPM&quot;)),
NOT($Permission.Incident_Reporting_Full_Access),
OR(
ISCHANGED(Reported_to_PHAROS__c) , 
ISCHANGED(Reported_to_The_Charity_Commission__c))
)</errorConditionFormula>
        <errorDisplayField>Reported_to_PHAROS__c</errorDisplayField>
        <errorMessage>Only SPMs can report incidents to PHAROS or The Charity Commission</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Severity_of_incident</fullName>
        <active>false</active>
        <description>Ensure a value of 1, 2, 3, 4, or 5 is entered</description>
        <errorConditionFormula>NOT(OR(
ISPICKVAL( Severity_of_incident_health__c ,&quot;1&quot;),
ISPICKVAL( Severity_of_incident_health__c ,&quot;2&quot;),
ISPICKVAL( Severity_of_incident_health__c ,&quot;3&quot;),
ISPICKVAL( Severity_of_incident_health__c ,&quot;4&quot;)))</errorConditionFormula>
        <errorMessage>Enter a value 4, 3, 2, or 1 to indicate the severity of the incident, where 4 is low and 1 is high</errorMessage>
    </validationRules>
