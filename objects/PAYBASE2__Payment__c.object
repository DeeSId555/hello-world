        <externalId>false</externalId>
        <formula>IF( NOT(ISBLANK( PAYBASE2__Created_From_Account__c )), PAYBASE2__Created_From_Account__r.Id, 
IF( NOT(ISBLANK( PAYBASE2__Created_From_Opportunity__c )), PAYBASE2__Created_From_Opportunity__r.AccountId, 
