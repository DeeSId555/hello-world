    <rules>
        <fullName>SL3a Place allocated Formula</fullName>
        <actions>
            <name>SL3a_place_allocated</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND( 
Access_category_filter__c = &quot;3a - extra mentor support for learning difficulties&quot; 
,ISCHANGED(Status__c), 
NOT(ISCHANGED(Access_category_filter__c)), 
OR( AND(ISPICKVAL( Status__c ,&quot;Assigned&quot;), NOT( ISPICKVAL( PRIORVALUE(Status__c ), &quot;Reserved&quot;) ) ) 
, 

AND(ISPICKVAL( Status__c, &quot;Reserved&quot;),NOT( ISPICKVAL( PRIORVALUE(Status__c ), &quot;Assigned&quot;) ) ) 

) 
, 
ISBLANK(Young_Person__r.Date_of_YPPG_withdrawal_notification__c ))</formula>
