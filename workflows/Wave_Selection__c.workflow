    <rules>
        <fullName>Was Previously HTR</fullName>
        <actions>
            <name>Was_Previously_HTR</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(  PRIORVALUE( HTR_Filter__c )= &quot;HTR YP&quot;  ,  ISCHANGED( HTR_Filter__c)  ,  ISPICKVAL( Inclusion_Detail__r.OBS_P3_form_status__c , &quot;Completed&quot;)  ,  NOT(HTR_Filter__c = &quot;HTR YP&quot; )  ,  OR(  ISPICKVAL( Status__c , &quot;Assigned&quot;)  ,  ISPICKVAL(Status__c, &quot;Reserved&quot;)  )  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Was Previously SL4a</fullName>
        <actions>
            <name>Was_previously_SL4</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(  PRIORVALUE( Access_category_filter__c )= &quot;4a  Extra APL support for YPs with restricted mobility, language barrier or learning difficulties&quot;  ,  ISCHANGED( Access_category_filter__c )  ,  ISPICKVAL( Inclusion_Detail__r.OBS_P3_form_status__c , &quot;Completed&quot;)  ,  NOT(Access_category__c = &quot;4a  Extra APL support for YPs with restricted mobility, language barrier or learning difficulties&quot; )  ,  OR(  ISPICKVAL( Status__c , &quot;Assigned&quot;)  ,  ISPICKVAL(Status__c, &quot;Reserved&quot;)  )  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Was Previously SL4b</fullName>
        <actions>
            <name>Was_previously_SL4b</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(  PRIORVALUE( Access_category_filter__c )= &quot;4b - extra APL support for challenging behaviour or mental health conditions&quot;  ,  ISCHANGED( Access_category_filter__c )  ,  ISPICKVAL( Inclusion_Detail__r.OBS_P3_form_status__c , &quot;Completed&quot;)  ,  NOT(Access_category__c = &quot;4b - extra APL support for challenging behaviour or mental health conditions&quot; )  ,  OR(  ISPICKVAL( Status__c , &quot;Assigned&quot;)  ,  ISPICKVAL(Status__c, &quot;Reserved&quot;)  )  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Was Previously SL5</fullName>
        <actions>
            <name>Was_Previously_SL5</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(  OR(PRIORVALUE( Access_category_filter__c )= &quot;5.1 - full-time support worker required&quot;  ,  PRIORVALUE( Access_category_filter__c )= &quot;5.05 - support worker required, can share with another YP&quot;  ,  PRIORVALUE( Access_category_filter__c )= &quot;5.2 - requires 2 full time support workers&quot;  ,  PRIORVALUE( Access_category_filter__c )= &quot;5b - personal coach required&quot;  )  ,  ISCHANGED( Access_category_filter__c )  ,  ISPICKVAL( Inclusion_Detail__r.OBS_P3_form_status__c , &quot;Completed&quot;)  ,  OR(NOT(Access_category__c = &quot;5.1 - full-time support worker required&quot; )  ,  NOT(Access_category__c = &quot;5.05 - support worker required, can share with another YP&quot; )  ,  NOT(Access_category__c = &quot;5.2 - requires 2 full time support workers&quot; )  ,  NOT(Access_category__c = &quot;5b - personal coach required&quot; )  ),  OR(  ISPICKVAL( Status__c , &quot;Assigned&quot;)  ,  ISPICKVAL(Status__c, &quot;Reserved&quot;)  )  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Was on all Girl Wave</fullName>
        <actions>
            <name>Was_on_All_Girl_Wave</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Removes wave name from Contact when YP is no longer assigned to a wave</description>
        <formula>AND(ISPICKVAL( PRIORVALUE(Status__c), &quot;Assigned&quot;),  ISPICKVAL( Wave__r.All_Girl_Wave__c , &quot;Yes&quot;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Wave assigned</fullName>
        <actions>
            <name>Update_NCS_programme</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Wave_Selection__c.Status__c</field>
            <operation>equals</operation>
            <value>Assigned</value>
        </criteriaItems>
        <description>This workflow will update a contact record with the wave that a YP is assigned to. This is for NCS MI monitoring purposes</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
