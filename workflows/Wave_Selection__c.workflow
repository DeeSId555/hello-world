        <fullName>Contact %27wave assigned%27%3A no longer assigned</fullName>
        <actions>
            <name>Assigned_wave_blank</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Removes wave name from Contact when YP is no longer assigned to a wave</description>
        <formula>AND( ISCHANGED(Status__c), ISPICKVAL(PRIORVALUE(Status__c ) , &quot;Assigned&quot;),(Young_Person__r.Assigned_waves__c = 1))</formula>
