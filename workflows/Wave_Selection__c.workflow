        </actions>
        <active>true</active>
        <formula>AND( OR (ISPICKVAL( Status__c , &quot;Withdrawn&quot;) , ISPICKVAL( Status__c , &quot;Autowithdrawn&quot;) , ISPICKVAL( Status__c , &quot;was reserved, gave up place&quot;) , ISPICKVAL( Status__c , &quot;Was assigned - gave up place&quot;) , ISPICKVAL( Status__c , &quot;Lost cause - removed from wave&quot;) , ISPICKVAL( Status__c , &quot;Unable to take on this wave for access reasons&quot;) , ISPICKVAL( Status__c , &quot;Moved to another NCS provider&quot;) , ISPICKVAL( Status__c , &quot;Not on wave, but was previously assigned or reserved&quot;) ) , OR(  ISPICKVAL(PRIORVALUE (Status__c),  &quot;Assigned&quot;) , ISPICKVAL(PRIORVALUE (Status__c),  &quot;Reserved&quot;) , ISPICKVAL(PRIORVALUE (Status__c),  &quot;Waiting List&quot;) ) , ISCHANGED(Status__c) )</formula>
        <triggerType>onAllChanges</triggerType>
