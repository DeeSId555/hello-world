            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_Date_Last_Activity</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Any_Call_made</name>
        <label>Any Call made?</label>
        <locationX>472</locationX>
        <locationY>506</locationY>
        <defaultConnector>
            <targetReference>Update_date_first_Call_attempt</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Call_has_been_made</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>DateFirstCallAttempt</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Was_Call_Successful</targetReference>
            </connector>
            <label>Call has been made</label>
        </rules>
    </decisions>
    <decisions>
        <name>Successful_Call_Already_made</name>
        <label>Successful Call Already made?</label>
        <locationX>1011</locationX>
        <locationY>506</locationY>
        <defaultConnector>
            <targetReference>Update_Date_first_Successful_Call</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Successful_Call_already_madeQ</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>DateFirstSuccessufulCall</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <label>Successful Call already made</label>
        </rules>
    </decisions>
    <decisions>
        <name>Was_Call_Successful</name>
        <label>Was Call Successful?</label>
        <locationX>766</locationX>
        <locationY>505</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Call_Successful</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>CallResult</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>No contact Wrong number</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>CallResult</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>No contact Dead number</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>CallResult</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>No contact Unable to leave Voicemail</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>CallResult</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Left voicemail</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Successful_Call_Already_made</targetReference>
            </connector>
            <label>Call Successful</label>
        </rules>
    </decisions>
