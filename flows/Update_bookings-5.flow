    <formulas>
        <name>ConvertToEndDT</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(
TEXT(YEAR({!BookingDate}))+&quot;-&quot;+
TEXT(MONTH({!BookingDate}))+&quot;-&quot;+
TEXT(DAY({!BookingDate}))+&quot; &quot;
+{!End_Time}&amp;&quot;:00&quot;)-(1/24)</expression>
    </formulas>
    <formulas>
        <name>ConvertToStartDT</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(
TEXT(YEAR({!BookingDate}))+&quot;-&quot;+
TEXT(MONTH({!BookingDate}))+&quot;-&quot;+
TEXT(DAY({!BookingDate}))+&quot; &quot;
+{!Start_Time}&amp;&quot;:00&quot;)-(1/24)</expression>
    </formulas>
