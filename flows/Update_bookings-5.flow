    <decisions>
        <name>Is_a_group_event</name>
        <label>Is a group event</label>
        <locationX>48</locationX>
        <locationY>249</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Is_Group_Event</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>GroupEvent</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>FindUsage</targetReference>
            </connector>
            <label>Is Group Event</label>
        </rules>
        <rules>
            <name>Not_Group_Event</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>GroupEvent</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Team_choice</targetReference>
            </connector>
            <label>Not Group Event</label>
        </rules>
    </decisions>
    <decisions>
        <name>Usage_Found</name>
        <label>Usage Found?</label>
        <locationX>558</locationX>
        <locationY>244</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Usage_already_exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>UsageID</leftValueReference>
                <operator>NotEqualTo</operator>
            </conditions>
            <connector>
                <targetReference>Lookup_booking</targetReference>
            </connector>
            <label>Usage already exists</label>
        </rules>
        <rules>
            <name>No_Usage_found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>UsageID</leftValueReference>
                <operator>EqualTo</operator>
            </conditions>
            <connector>
                <targetReference>CreateUsage</targetReference>
            </connector>
            <label>No Usage found</label>
        </rules>
    </decisions>
