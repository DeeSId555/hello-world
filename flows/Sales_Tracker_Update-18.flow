            </connector>
            <label>Record Found</label>
        </rules>
    </decisions>
    <formulas>
        <name>NameSalesTracker</name>
        <dataType>String</dataType>
        <expression>LEFT({!SchoolName},40)&amp;TEXT(DAY({!Date}))&amp;&quot;/&quot;&amp;TEXT(MONTH({!Date}))&amp;&quot;/&quot;&amp;TEXT(YEAR({!Date}))</expression>
    </formulas>
    <formulas>
